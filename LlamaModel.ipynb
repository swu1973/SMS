        "  format = \"\"\n",
        "  for item in lst:\n",
        "    try:\n",
        "      topics = item.split(' - ')[0]\n",
        "      subtopic_split = item.split(' - ')[1]\n",
        "      subtopics = subtopic_split.split('|')\n",
        "      format += topics+':'+str(subtopics)+'|'\n",
        "    except:\n",
        "      pass\n",
